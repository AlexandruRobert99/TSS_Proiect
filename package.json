{
  "devDependencies": {
    "cypress": "^13.8.1",
    "cypress-parallel": "^0.14.0"
  },

  "scripts": {
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:parallel:chrome": "cypress-parallel -s cy:run:chrome -t 2 -d cypress/e2e/testare_paralela",
    "cy:parallel:firefox": "cypress-parallel -s cy:run:firefox -t 2 -d cypress/e2e/testare_paralela",
    "cy:parallel:edge": "cypress-parallel -s cy:run:edge -t 2 -d cypress/e2e/testare_paralela"
  }  
  
}
